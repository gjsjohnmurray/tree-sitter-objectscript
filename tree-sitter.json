{
  "grammars": [
    {
      "name": "objectscript",
      "camelcase": "ObjectScript",
      "scope": "source.objectscript",
      "path": "udl",
      "external-files": [
      ],
      "file-types": [
        "cls"
      ],
      "highlights": [
        "expr/queries/highlights.scm",
        "core/queries/highlights.scm",
        "udl/queries/highlights.scm"
      ],
      "injections": [
        "expr/queries/injections.scm",
        "core/queries/injections.scm",
        "udl/queries/injections.scm"
      ],
      "locals": [
        "expr/queries/locals.scm",
        "core/queries/locals.scm",
        "udl/queries/locals.scm"
      ],
      "tags": [
      ],
      "injection-regex": "^(cos|objectscript)$"
    }
  ],
  "metadata": {
    "version": "0.0.20",
    "license": "MIT",
    "description": "ObjectScript (UDL) grammar for tree-sitter",
    "authors": [
      {
        "name": "Dave McCaldon",
        "email": "davem@intersystems.com"
      },
      {
        "name": "Michael Fortunato",
        "email": "michael.n.fortunato@gmail.com"
      },
      {
        "name": "Marc Johnson",
        "email": "marc.johnson@intersystems.com"
      }
    ],
    "links": {
      "repository": "https://github.com/intersystems/tree-sitter-objectscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true
  }
}
